{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "quotes": 0,
        "import/extensions": 0,
        "no-param-reassign": 0,
        "no-console" : 0,
        "max-classes-per-file": 0
    }
}
